<Window x:Class="Client.WarningRes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="Warning" Height="170" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,30,0,0" FontSize="20" >File not found!</TextBlock>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,0,10" FontSize="20" >Will be remove from list</TextBlock>
            <Button Name="CloseButton" Height="30" Width="80" Click="Close_Click">Close</Button>
        </StackPanel>
    </Grid>
</Window>
